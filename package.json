{
  "name": "bazzite-changelogs",
  "version": "0.0.4",
  "description": "A plugin that swaps SteamOS' update changelogs for Bazzite's changelogs.",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victor-borges/bazzite-changelogs.git"
  },
  "keywords": [
    "bazzite",
    "release-notes",
    "release",
    "changelogs",
    "changes"
  ],
  "author": "Victor Borges <victor1882@outlook.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/victor-borges/bazzite-changelogs/issues"
  },
  "homepage": "https://github.com/victor-borges/bazzite-changelogs#readme",
  "devDependencies": {
    "@decky/rollup": "^1.0.1",
    "@decky/ui": "^4.7.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/webpack": "^5.28.5",
    "rollup": "^4.22.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@decky/api": "^1.1.2",
    "async-mutex": "^0.5.0",
    "decky-frontend-lib": "^3.25.0",
    "react-icons": "^5.3.0",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "tslib": "^2.7.0",
    "unified": "^11.0.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
